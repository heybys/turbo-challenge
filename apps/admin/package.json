{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite --mode on-local",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx,.js .",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js .",
    "build:local": "tsc && vite build --mode on-local",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "clean": "rm -rf ./node_modules && npm run clean:build",
    "clean:build": "rm -rf ./dist && rm -rf ./.turbo",
    "preview": "tsc && vite build --mode on-local && vite preview",
    "dockerize": "docker-compose up -d",
    "clean:dockerize": "docker-compose down && docker rmi turbo-challenge"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.2.0",
    "@okta/okta-react": "^6.7.0",
    "@repo/ui": "*",
    "@tanstack/react-query": "^5.29.2",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "web-vitals": "^3.5.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.29.2",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "jest-styled-components": "^7.2.0",
    "msw": "^2.2.13",
    "styled-components": "^6.1.8",
    "typescript": "^5.3.3",
    "vite": "^5.2.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
